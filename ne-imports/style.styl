/* Form Defaults  */
/* <><><><><><><><><><><><><><><><><> */
/* <><><><><><><><><><><><><><><><><> */

input
  border: inset
  margin-left 1%
  margin-right 2%
  border-radius 5px
  width 95%
  border-color #535d65
  padding-left 1%
  padding-right 1%
  color grey

input:focus
  border inset
  border-color black
  color black

textarea
  padding-left 1%
  padding-right 1%
  margin-left 1%
  margin-right 2%
  border-radius 5px
  width 95%
  color grey

textarea:focus
  border inset
  border-color black
  color black

select
  margin-left 1%
  margin-right 2%
  border-radius 5px
  width 97%
  text-align center
  color grey

select:focus
  border inset
  border-color black
  color black


